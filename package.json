{
  "name": "sodiumjs",
  "version": "0.9.0",
  "description": "A Functional Reactive Programming (FRP) library for JavaScript",
  "author": "Stephen Blackheath",
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/SodiumFRP/sodium-typescript",
  "repository": {
    "type": "git",
    "url": "https://github.com/SodiumFRP/sodium-typescript.git"
  },
  "bugs": {
    "url": "https://github.com/SodiumFRP/sodium-typescript/issues"
  },
  "keywords": [
    "frp",
    "functional",
    "reactive",
    "typescript",
    "sodium"
  ],
  "main": "./dist/lib/Sodium.js",
  "typings": "./dist/lib/Sodium.d.ts",
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build:umd": "webpack --progress --colors --mode=dev && rimraf ./dist/lib/src",
    "build": "npm run prebuild && npm run build:umd && tsc",
    "prerelease": "npm run build",
    "test": "node ./dist/test/suite.js && node ./dist/test/test-timers.js",
    "karma": "karma start"
  },
  "dependencies": {
    "typescript-collections": "github:rixrix/typescript-collections"
  },
  "devDependencies": {
    "@types/jasmine": "^2.2.30",
    "@types/node": "^4.0.30",
    "@types/source-map": "^0.1.26",
    "@types/uglify-js": "^2.0.27",
    "@types/webpack": "^1.12.30",
    "awesome-typescript-loader": "^2.1.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.1.2",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "rimraf": "^2.5.3",
    "typescript": "^1.8.10",
    "uglify-js": "^2.7.0",
    "webpack": "^1.13.1"
  }
}
